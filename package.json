{
  "name": "securityautofill-trainingdata",
  "description": "training data for security autofill's setfit model",
  "version": "1.0.1",
  "author": "Lucas Johnston <lucas@lucasjohnston.co.uk>",
  "private": "true",
  "main": "src/index.ts",
  "scripts": {
    "start": "npx ts-node -r tsconfig-paths/register src/index.ts",
    "dev": "export DEBUG=\"*\" && ts-node-dev -r tsconfig-paths/register --respawn src/index.ts",
    "build": "tsc",
    "update": "npx npm-check-updates -p yarn -u"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/deep-equal": "^1.0.4",
    "@types/errorhandler": "^1.5.3",
    "@types/node": "^20.10.5",
    "@types/node-fetch": "^2.6.10",
    "@types/uuid": "^9.0.7",
    "@types/validator": "^13.11.7",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "axios": "^1.6.3",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.3",
    "csv-writer": "^1.6.0",
    "date-fns": "^3.0.6",
    "deep-equal": "^2.2.3",
    "errorhandler": "^1.5.1",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.2",
    "moment": "^2.30.1",
    "node-fetch": "^3.3.2",
    "puppeteer": "^21.6.1",
    "puppeteer-cluster": "^0.23.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-recaptcha": "^3.6.8",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "ts-enums": "^0.0.6",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "uuid": "^9.0.1",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  }
